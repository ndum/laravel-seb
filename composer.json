{
    "name": "ndum/laravel-seb",
    "description": "Laravel Package for generating encrypted SEB (Safe Exam Browser) client configurations",
    "type": "laravel",
    "keywords": [
        "laravel",
        "SEB",
        "Safe Exam Browser",
        "Exams",
        "Exam Configurations",
        "xml",
        "plist"
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ndum/CFPropertyList"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-openssl": "*",
        "ext-zlib": "*",
        "illuminate/support": "^8.64",
        "illuminate/collections": "^8.64",
        "rodneyrehm/plist": "dev-seb-fix"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas D",
            "email": "nd@nidum.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ndum\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Ndum\\Laravel\\SebConfigGeneratorServiceProvider"
            ],
            "aliases": {
                "SebConfigGenerator": "Ndum\\Laravel\\Facades\\SebConfigGenerator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
